{
  "name": "@repo/shared",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "scripts": {
    "typegen:db": "kanel -c ./db.kanelrc.cjs && prettier --write ./generated/db/*",
    "routegen": "tsx ./scripts/generateApiFetchTypes",
    "typecheck": "tsc --noEmit",
    "lint": "eslint",
    "format": "prettier --write .",
    "build": "tsc"
  },
  "exports": {
    "./generated/*": {
      "types": "./dist/generated/*.d.ts",
      "default": "./dist/generated/*.js"
    },
    "./testHelpers/*": {
      "types": "./dist/testHelpers/*.d.ts",
      "default": "./dist/testHelpers/*.js"
    },
    "./types/*": {
      "types": "./dist/types/*.d.ts",
      "default": "./dist/types/*.js"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.6.2",
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@faker-js/faker": "^9.5.1",
    "@ianvs/prettier-plugin-sort-imports": "^4.4.1",
    "@types/node": "^22.13.7",
    "bcryptjs": "^3.0.2",
    "date-fns": "^4.1.0",
    "eslint": "^9.21.0",
    "eslint-plugin-import": "^2.31.0",
    "globals": "^16.0.0",
    "kanel": "^3.11.1",
    "kanel-zod": "^1.4.0",
    "postgres-interval": "^4.0.2",
    "prettier": "3.5.3",
    "ts-morph": "^25.0.1",
    "tsx": "^4.19.3",
    "typescript-eslint": "^8.27.0"
  },
  "dependencies": {
    "postgres-range": "^1.1.4",
    "zod": "^3.24.2"
  }
}